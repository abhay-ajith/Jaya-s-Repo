String pass="1234";
String pass1;
void setup()
{
  Serial.begin(9600);

}

void loop()
{
  if(Serial.available())
  {
  pass1= Serial.readString();
  if (pass1==pass) {
    Serial.println("crt");
  } else {
    Serial.println("wrng");
  }
  }  
  delay(10); // Delay a little bit to improve simulation performance
}
